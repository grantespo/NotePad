<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="noteapp.cloudnotebook.grantespo.notesprocloudnotepad">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

    <application
        android:name="noteapp.cloudnotebook.grantespo.notesprocloudnotepad.ConnectToParse"
        android:allowBackup="true"
        android:icon="@drawable/skynotelogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/SkyTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name="noteapp.cloudnotebook.grantespo.notesprocloudnotepad.ThemeActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="noteapp.cloudnotebook.grantespo.notesprocloudnotepad.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="noteapp.cloudnotebook.grantespo.notesprocloudnotepad.CreateNote"
            android:screenOrientation="portrait">

        </activity>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-1903987913186752~1045432050"/>
    </application>

</manifest>